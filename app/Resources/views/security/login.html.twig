{% extends 'base.html.twig' %}

{% block body %}

    {{ parent() }}

    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}

    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}

    <button type="submit">login</button>
    {{ form_end(form) }}

{% endblock %}